<%- include('partials/header') -%>
    <div class="flex flex-col min-h-screen backdrop-grayscale-0 bg-center bg-cover py-20"
        style="background-image: url(https://images.unsplash.com/photo-1507608158173-1dcec673a2e5?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=870&q=80);">
        <div class="max-w-3xl w-11/12 mx-auto z-10 flex text-white items-center justify-between">
            <div class="flex items-center py-4">
                <h2 class="text-xl">Welcome <%=name %>
                </h2>
                <a class="flex items-center nav-link pr-2 lg:px-2 py-2 text-gray-600 hover:text-gray-700 focus:text-gray-700 transition duration-150 ease-in-out"
                    href="/auth/signout" data-mdb-ripple="true" data-mdb-ripple-color="light">
                    <svg class="h-4 w-4 mr-1" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                        stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                        <path stroke-linecap="round" stroke-linejoin="round"
                            d="M15.75 9V5.25A2.25 2.25 0 0013.5 3h-6a2.25 2.25 0 00-2.25 2.25v13.5A2.25 2.25 0 007.5 21h6a2.25 2.25 0 002.25-2.25V15M12 9l-3 3m0 0l3 3m-3-3h12.75" />
                    </svg>
                    Sign Out</a>
            </div>
            <div class="flex gap-4 items-center">
                <p class="">Stores: <%=stores.length %>
                </p>
                <a href="/stores/add">
                    <button type="button"
                        class="inline-block px-6 py-2.5 bg-cyan-600 text-white font-medium text-xs leading-tight uppercase rounded shadow-md hover:bg-cyan-700 hover:shadow-lg focus:bg-cyan-700 focus:shadow-lg focus:outline-none focus:ring-0 active:bg-cyan-800 active:shadow-lg transition duration-150 ease-in-out"
                        data-mdb-ripple="true" data-mdb-ripple-color="light">Add New Store
                    </button>
                </a>
            </div>
        </div>
        <div class="absolute bg-black h-full opacity-75 inset-0 z-0"></div>
        <div class="max-w-3xl w-full mx-auto z-10">
            <div class="flex flex-col">
                <% for(let i=0; i<stores.length; i++) {%>
                    <div class="bg-gray-900 border border-gray-900 shadow-lg  rounded-3xl p-4 m-4">
                        <div class="flex-none sm:flex">
                            <div class="relative h-32 w-32 sm:mb-0 mb-3">
                                <img src="https://images.unsplash.com/photo-1604066867775-43f48e3957d8?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=870&q=80"
                                    alt="aji" class=" w-32 h-32 rounded-2xl">
                            </div>
                            <div class="flex-auto sm:ml-5 justify-evenly">
                                <div class="flex items-center justify-between sm:mt-2">
                                    <div class="flex items-center">
                                        <div class="flex flex-col">
                                            <div class="w-full flex-none text-lg text-gray-200 font-bold leading-none">
                                                <%=stores[i].name %>
                                            </div>
                                            <div class="flex-auto text-gray-400 my-1">
                                                <span class="mr-3 py-2">
                                                    <%=stores[i].location %>
                                                </span><span
                                                    class="mr-3 border-r border-gray-600  max-h-0"></span><span>
                                                    <%=stores[i].createdAt %>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="flex flex-row items-center">
                                    <div class="flex">
                                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor"
                                            class="h-5 w-5 text-yellow-400">
